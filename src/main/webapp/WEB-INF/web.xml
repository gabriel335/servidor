<web-app>
    <servlet>
        <servlet-name>init-auth</servlet-name>
        <servlet-class>ar.incluit.fintech.anses.servidor.web.servlet.InitAuth</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>init-auth</servlet-name>
        <url-pattern>/init-auth</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>cors</filter-name>
        <filter-class>ar.incluit.fintech.anses.servidor.web.CorsFilter</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>cors</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

</web-app>
